#!/bin/bash

iptables -t nat -D OUTPUT -j TORO2CHAIN_OUTPUT
iptables -D OUTPUT -j TORO2CHAIN_OUTPUT
iptables -D INPUT -j TORO2CHAIN_INPUT

iptables -t nat -F TORO2CHAIN_OUTPUT
iptables -t nat -X TORO2CHAIN_OUTPUT
iptables -F TORO2CHAIN_INPUT
iptables -X TORO2CHAIN_INPUT
iptables -F TORO2CHAIN_OUTPUT
iptables -X TORO2CHAIN_OUTPUT

if [ $? -eq 0 ]; then
  echo "Removing new toro2 rules: SUCCESS"
  exit 0;
else
  echo "Removing new toro2 rules: FAILURE"
  exit 1;
fi;
